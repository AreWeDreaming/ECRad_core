#!/bin/bash -l
#SBATCH -e ECRad.err # standard error file
#SBATCH -o ECRad.out # standard output file
#SBATCH --export=ALL
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --wait

export OMP_NUM_THREADS=${SLURM_CPUS_PER_TASK:-1}

# For pinning threads correctly:
export OMP_PLACES=cores 

srun $ECRad $ECRad_working_dir_1